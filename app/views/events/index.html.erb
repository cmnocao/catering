<h3 class="page-header">
  Events <small> | </small><span class="badge"><%= @events_count.count %></span>
</h3>
<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
		<% @events.each do |e| %>
			<tr>
				<td>	
					<%= e.name %>
				</td>
			</tr>
		<% end %>
	</tbody>
</table>