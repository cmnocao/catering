<h3 class="page-header">
  Venues <small> | </small><span class="badge"><%= @venues.count %></span>
  <div class="pull-right">
    <%= link_to new_venue_path do %>
      <i class="fa fa-plus-circle"></i>
    <% end %>
  </div>
</h3>
<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <% @venues.each do |v| %>
      <tr>
        <td><b><%= link_to v.name, venue_path(v) %></b></td>
      </tr>
    <% end %>
  </tbody>
</table>